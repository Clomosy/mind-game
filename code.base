  procedure ClosingGame
  begin
  Dec(SeeResultSecond)
  if FrmAd ='Lvl1Form' then
  begin
   Lvl1SureLbl.Text:=IntToStr(SeeResultSecond)+' Saniye';
   if SeeResultSecond = 0 then
   begin
     FrmLoginScreen.ClShow;
     Lvl1Form.ClHide;
     SeeResultTime.Enabled:=False;
   end;
  end;
  if FrmAd ='Lvl2Form' then
  begin
   Lvl2SureLbl.Text:=IntToStr(SeeResultSecond)+' Saniye';
   if SeeResultSecond = 0 then
   begin
     FrmLoginScreen.ClShow;
     Lvl2Form.ClHide;
     SeeResultTime.Enabled:=False;
   end;
  end;
  if FrmAd ='Lvl3Form' then
  begin
  Lvl3SureLbl.Text:=IntToStr(SeeResultSecond)+' Saniye';
   if SeeResultSecond = 0 then
   begin
     FrmLoginScreen.ClShow;
     Lvl3Form.ClHide;
     SeeResultTime.Enabled:=False;
   end;
  end;
  end;
    
  procedure AnswerTimeOver
  begin
    Dec(AnswerSecond)  
    if FrmAd = 'Lvl1Form' then
    begin
    Lvl1SureLbl.Text := IntToStr(AnswerSecond)+' Saniye';
        if AnswerSecond = 0 then                        
        begin
        AnswerTime.Enabled:=False;
        Controls;
        Lvl1ControlBtn.Visible:=False;
        Lvl1ShowTrueBtn.Visible:=True;
        Lvl1ShowAgainBtn.Enabled:=False;
        for syc:= 1 to 2 do
         begin
          TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).ReadOnly := True;
         end;
        SeeResultTime:= Lvl1Form.AddNewTimer(Lvl1Form,'SeeResultTime',1000);
        SeeResultTime.Enabled := True;
        SeeResultSecond :=7;
        if LeftToFinish <> 0 then
        begin
        Lvl1Form.PlayGameSound(LoseSound);
        ShowMessage('10 Saniye içinde doğru cevap veremediğiniz için oyun bitti . ');
        end;
        end;
    end;    
    if FrmAd = 'Lvl2Form' then
    begin
    Lvl2SureLbl.Text := IntToStr(AnswerSecond)+' Saniye';
        if AnswerSecond = 0 then                        
        begin
        AnswerTime.Enabled:=False;
        Controls;
        Lvl2ControlBtn.Visible:=False;
        Lvl2ShowTrueBtn.Visible:=True;
        Lvl2ShowAgainBtn.Enabled:=False;
        for syc:= 1 to 4 do
         begin
          TclProEdit(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Edt')).ReadOnly := True;
         end;
        SeeResultTime:= Lvl2Form.AddNewTimer(Lvl2Form,'SeeResultTime',1000);
        SeeResultTime.Enabled := True;
        SeeResultSecond :=10;
        if LeftToFinish <> 0 then
        begin
        Lvl2Form.PlayGameSound(LoseSound);
        ShowMessage('15 Saniye içinde doğru cevap veremediğiniz için oyun bitti . ');
        end;
        end;
    end;    
    if FrmAd = 'Lvl3Form' then
    begin
    Lvl3SureLbl.Text := IntToStr(AnswerSecond)+' Saniye';
        if AnswerSecond = 0 then                        
        begin
        AnswerTime.Enabled:=False;
        Controls;
        Lvl3ControlBtn.Visible:=False;
        Lvl3ShowTrueBtn.Visible:=True;
        Lvl3ShowAgainBtn.Enabled:=False;
        for syc:= 1 to 7 do
         begin
          TclProEdit(Lvl3Form.clFindComponent('Lvl3Number'+IntToStr(syc)+'Edt')).ReadOnly := True;
         end;
        SeeResultTime:= Lvl3Form.AddNewTimer(Lvl3Form,'SeeResultTime',1000);
        SeeResultTime.Enabled := True;
        SeeResultSecond :=15;
        if LeftToFinish <> 0 then
        begin
        Lvl3Form.PlayGameSound(LoseSound);
        ShowMessage('20 Saniye içinde doğru cevap veremediğiniz için oyun bitti . ');
        end;
        end;
    end;  
  end;
  
  procedure TimeOver
  begin             
    Dec(ShowSecond)     
    if FrmAd = 'Lvl1Form' then
    begin
    Lvl1SureLbl.Text := IntToStr(ShowSecond)+' Saniye';
    if ShowSecond = 0 then                        
    begin
    ShowTime.Enabled:=False;
    AnswerTime.Enabled := True;
    AnswerSecond :=10;
    Lvl1SureLbl.Text:='10 Saniye';
    Lvl1Form.AddNewEvent(AnswerTime,tbeOnTimer,'AnswerTimeOver');
        for syc:= 1 to 2 do
         begin
          TclProLabel(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Lbl')).Visible := False;
          TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).Visible := True;
         end;
         Lvl1PointLbl.Visible:=True;
         Lvl1ControlBtn.Enabled:=True;
         if RightOver = False then
         begin
           Lvl1ShowAgainBtn.Enabled:=True;
           RightOver := True;
         end;
         else
         begin
           Lvl1ShowAgainBtn.Enabled:=False;
         end;
    end;
    end;
    if FrmAd = 'Lvl2Form' then
    begin
    Lvl2SureLbl.Text := IntToStr(ShowSecond)+' Saniye';
    if ShowSecond = 0 then                        
    begin
    ShowTime.Enabled:=False;
    AnswerTime.Enabled := True;
    AnswerSecond :=15;
    Lvl2SureLbl.Text:='15 Saniye';
    Lvl2Form.AddNewEvent(AnswerTime,tbeOnTimer,'AnswerTimeOver');
        for syc:= 1 to 4 do
         begin
          TclProLabel(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Lbl')).Visible := False;
          TclProEdit(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Edt')).Visible := True;
         end;
         Lvl2PointLbl.Visible:=True;
         Lvl2ControlBtn.Enabled:=True;
         if RightOver = False then
         begin
           Lvl2ShowAgainBtn.Enabled:=True;
           RightOver := True;
         end;
         else
         begin
           Lvl2ShowAgainBtn.Enabled:=False;
         end;
    end;
    end;
    if FrmAd = 'Lvl3Form' then
    begin
    Lvl3SureLbl.Text := IntToStr(ShowSecond)+' Saniye';
    if ShowSecond = 0 then                        
    begin
    ShowTime.Enabled:=False;
    AnswerTime.Enabled := True;
    AnswerSecond :=20;
    Lvl3SureLbl.Text:='20 Saniye';
    Lvl3Form.AddNewEvent(AnswerTime,tbeOnTimer,'AnswerTimeOver');
        for syc:= 1 to 7 do
         begin
          TclProLabel(Lvl3Form.clFindComponent('Lvl3Number'+IntToStr(syc)+'Lbl')).Visible := False;
          TclProEdit(Lvl3Form.clFindComponent('Lvl3Number'+IntToStr(syc)+'Edt')).Visible := True;
         end;
         Lvl3PointLbl.Visible:=True;
         Lvl3ControlBtn.Enabled:=True;
         if RightOver = False then
         begin
           Lvl3ShowAgainBtn.Enabled:=True;
           RightOver := True;
         end;
         else
         begin
           Lvl3ShowAgainBtn.Enabled:=False;
         end;
    end;
  end;  
  end;
  
  procedure AgainJkr
  begin
  if FrmAd = 'Lvl1Form' then
  begin
    Right:=Right-1;
    Lvl1RightLbl.Text:='Hak : '+IntToStr(Right);
    for syc:= 1 to 2 do
    begin
      if TclProLabel(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Lbl')).Text = TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).Text then
      begin
       TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).ReadOnly:=True;
      end;
      else
      begin
        TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).ReadOnly := False;;
      end;
      TclProLabel(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Lbl')).Visible := True;
      TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).Visible := False;
    end;
    Lvl1PointLbl.Visible:=False;
    Lvl1ControlBtn.Enabled:=False;
    Lvl1ShowAgainBtn.Enabled:=False;
    AnswerTime.Enabled:=False;
    ShowTime.Enabled:=True;
    Lvl1SureLbl.Text :='5 Saniye';
    ShowSecond:=5;
    Dec(ShowSecond)
    Lvl1SureLbl.Text := IntToStr(ShowSecond)+' Saniye';
    if ShowSecond = 0 then
    begin
     ShowTime.Enabled:=False;
     AnswerTime.Enabled:=True;
     AnswerTimeOver;
    end; 
  end;  
  if FrmAd = 'Lvl2Form' then
  begin
    Right:=Right-1;
    Lvl2RightLbl.Text:='Hak : '+IntToStr(Right);
    for syc:= 1 to 4 do
    begin
      if TclProLabel(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Lbl')).Text = TclProEdit(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Edt')).Text then
      begin
       TclProEdit(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Edt')).ReadOnly:=True;
      end;
      else
      begin
        TclProEdit(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Edt')).ReadOnly := False;;
      end;
      TclProLabel(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Lbl')).Visible := True;
      TclProEdit(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Edt')).Visible := False;
