    procedure CloseGame
    begin
      TclProButton(FrmLoginScreen.clFindComponent('BtnGoBack')).Click;
    end;
    
    procedure GoBack
    begin
     ShowTime.Enabled:=False;
     AnswerTime.Enabled:=False;
     if FrmAd = 'Lvl1Form' then 
     begin
     FrmLoginScreen.ClShow;
     Lvl1Form.ClHide;
     end;
     else if FrmAd = 'Lvl2Form' then 
     begin
     FrmLoginScreen.ClShow;
     Lvl1Form.ClHide;
     Lvl2Form.ClHide;
     end;
     else if FrmAd = 'Lvl3Form' then 
     begin
     FrmLoginScreen.ClShow;
     Lvl1Form.ClHide;
     Lvl2Form.ClHide;
     Lvl3Form.ClHide;
     end;
    end;
    
    procedure VirtualKeyboardShow
    begin
      if FrmAd = 'Lvl1Form' then
      begin
      Lvl1BottomLyt.Margins.Bottom:=Lvl1Form.clVKBoundsHeight;
      end;
      if FrmAd = 'Lvl2Form' then
      begin
      Lvl2BottomLyt.Margins.Bottom:=Lvl2Form.clVKBoundsHeight;
      end;
      if FrmAd = 'Lvl3Form' then
      begin
      Lvl3BottomLyt.Margins.Bottom:=Lvl3Form.clVKBoundsHeight;
      end;
    end;
    
    procedure VirtualKeyboardHidden
    begin
      if FrmAd = 'Lvl1Form' then
      begin
      Lvl1BottomLyt.Margins.Bottom:=0;
      end;
      if FrmAd = 'Lvl2Form' then
      begin
      Lvl2BottomLyt.Margins.Bottom:=0;
      end;
      if FrmAd = 'Lvl3Form' then
      begin
      Lvl3BottomLyt.Margins.Bottom:=0;
      end;
    end;
    
    procedure ShowTrue
    begin
     if FrmAd = 'Lvl1Form' then
     begin
     Lvl1StartBtn.Visible:=False;
     Lvl1ShowAgainBtn.Visible:=False;
     Lvl1ControlBtn.Visible:=False;
     Lvl1ShowTrueBtn.Enabled:=False;
     
     if LeftToFinish <> 0 then
     begin
     Lvl1Form.AddNewEvent(SeeResultTime,tbeOnTimer,'ClosingGame');
     end;
     
     for syc:= 1 to 2 do
     begin
     TclProLabel(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Lbl')).Visible := True;
     TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).Visible := False;
     end;
     end;
     if FrmAd = 'Lvl2Form' then
     begin
     Lvl2StartBtn.Visible:=False;
     Lvl2ShowAgainBtn.Visible:=False;
     Lvl2ControlBtn.Visible:=False;
     Lvl2ShowTrueBtn.Enabled:=False;
     
     if LeftToFinish <> 0 then
     begin
     Lvl2Form.AddNewEvent(SeeResultTime,tbeOnTimer,'ClosingGame');
     end;
     
     for syc:= 1 to 4 do
     begin
     TclProLabel(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Lbl')).Visible := True;
     TclProEdit(Lvl2Form.clFindComponent('Lvl2Number'+IntToStr(syc)+'Edt')).Visible := False;
     end;
     end;
    if FrmAd = 'Lvl3Form' then
    begin
     Lvl3StartBtn.Visible:=False;
     Lvl3ShowAgainBtn.Visible:=False;
     Lvl3ControlBtn.Visible:=False;
     Lvl3ShowTrueBtn.Enabled:=False;
     
     if LeftToFinish <> 0 then
     begin
     Lvl3Form.AddNewEvent(SeeResultTime,tbeOnTimer,'ClosingGame');
     end;
     
     for syc:= 1 to 7 do
     begin
     TclProLabel(Lvl3Form.clFindComponent('Lvl3Number'+IntToStr(syc)+'Lbl')).Visible := True;
     TclProEdit(Lvl3Form.clFindComponent('Lvl3Number'+IntToStr(syc)+'Edt')).Visible := False;
     end;
    end; 
    end;
  
    procedure Controls
    begin
    TotalScore:=0;
    if FrmAd = 'Lvl1Form' then
    begin
    LeftToFinish:=2;
      for syc:= 1 to 2 do
      begin
        if TclProLabel(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Lbl')).Text = TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).Text then
        begin
         TotalScore:=TotalScore+10;
         clComponent.SetupComponent(TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')),'{"BackgroundColor":"#00FF00"}');
         TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).ReadOnly:=True;
         LeftToFinish:=LeftToFinish-1;
         if LeftToFinish = 0 then 
         begin
          Lvl1Form.PlayGameSound(WonSound);
          Lvl1NewChapter.Visible:=True;
          Lvl1StartBtn.Visible:=False;
          Lvl1ShowAgainBtn.Visible:=False;
          Lvl1ControlBtn.Visible:=False;
          AnswerTime.Enabled:=False;
         end;
        end;
        else if TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')).Text = '' then
        begin
         clComponent.SetupComponent(TclProEdit(Lvl1Form.clFindComponent('Lvl1Number'+IntToStr(syc)+'Edt')),'{"BackgroundColor":"#C0C0C0"}');
        end;
        else
